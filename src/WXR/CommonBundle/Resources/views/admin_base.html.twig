  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

    <title>{% block title %}Admin Panel{% endblock %}</title>

    <meta name="viewport" content="width=device-width">

    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    <link rel="stylesheet" href="{{ asset('css/main.css') }}">
    {% block stylesheets %}{% endblock %}

    <script src="{{ asset('js/libs/modernizr-2.5.3-respond-1.1.0.min.js') }}"></script>
  </head>
  <body>

    {% block body %}{% endblock %}

    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js"></script>
    <script type="text/javascript">
      window.jQuery || document.write('<script src="{{ asset('js/libs/jquery-1.7.2.min.js') }}"><\/script>');
    </script>
    <script src="{{ asset('js/libs/bootstrap/bootstrap.min.js') }}"></script>
    <script src="{{ asset('js/main.js') }}"></script>

    {% block javascripts %}{% endblock %}
  </body>
</html>
